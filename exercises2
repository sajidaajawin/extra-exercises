var number1 = prompt("Enter a number:");
alert("You entered: " + number1);
var number2 = prompt("Enter the first number:");
var number3 = prompt("Enter the second number:");

var min = Math.min(number2, number3);
var max = Math.max(number2, number3);

alert("Numbers in ascending order: " + min + ", " + max);

var number4 = prompt("Enter the first number:");
var number5 = prompt("Enter the second number:");

var largerNumber = Math.max(number4, number5);
alert("The larger number is: " + largerNumber);

var number6 = prompt("Enter the first number:");
var number7 = prompt("Enter the second number:");

var sum = parseInt(number6) + parseInt(number7);
alert("The sum of " + number6 + " and " + number7 + " is: " + sum);
